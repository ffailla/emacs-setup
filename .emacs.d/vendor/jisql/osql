#!/bin/sh

export CLASSPATH=$CLASSPATH:~/.m2/repository/com/microsoft/sqljdbc4/3.0/sqljdbc4-3.0.jar:~/.emacs.d/site-lisp/jisql/lib/jisql-2.0.7.jar:~/.emacs.d/site-lisp/jisql/lib/jopt-simple-3.2.jar:~/.emacs.d/site-lisp/jisql/lib/javacsv.jar

java -server -Xmx1G -cp $CLASSPATH com.xigole.util.sql.Jisql -user $4 -password $2 -driver com.microsoft.sqlserver.jdbc.SQLServerDriver -cstring jdbc:sqlserver://$8:1433\;database=$6 
